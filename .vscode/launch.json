	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"protocol": "inspector",
			"runtimeArgs": [
				"run", "devstart"
			],
			"restart": true,
			"console": "integratedTerminal",
			"env": {
				"DEBUG": "maces:*",
				"NODE_ENV": "development"
			},
			"port": 5858,
		}
	]
}
